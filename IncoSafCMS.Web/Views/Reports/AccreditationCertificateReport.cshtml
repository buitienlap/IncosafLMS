@model AccreditationCertificateReport

@{ 
    Layout = "~/Views/Shared/_ReportLayout.cshtml";
    ViewBag.Title = "Chứng nhận kiểm định";
}

@Html.DevExpress().WebDocumentViewer(settings =>
{
    settings.Name = "AccreditationCertificateReportWebViewer";
    settings.Width = System.Web.UI.WebControls.Unit.Percentage(100);
    settings.Height = System.Web.UI.WebControls.Unit.Percentage(100);
    settings.ClientSideEvents.Init = "function(s, e){ setTimeout(function(){ s.previewModel.reportPreview.zoom(1); s.previewModel.reportPreview.showMultipagePreview(true);}, 1); }";
}).Bind(Model).GetHtml()

